	fbPRG_XYZ_FillQueue_LIN3D_with_Array_SFC		: PRG06_XYZ_FillQueue_LIN3D_with_Array_SFC;
	fbPRG_XYZ_USER_ST									: PRG07_XYZ_USER_ST;
	fbPRG_XYZ_USER_SFC									: PRG08_XYZ_USER_SFC;
	InstanceNumber: INT;
END_VAR
